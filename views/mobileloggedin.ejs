            <h2 class="text-clipped text-center">MOBILE PHONES</h2>
            <div class="shop-items" style="margin-left: 30px; margin-right: 30px;">
                <% result.forEach(function(result){ %>
                <div class="shop-item" data-item-id="<%= result.id %>">
                    <span class="shop-item-title"><%=result.name%></span>
                    <img class="shop-item-image" src="<%= result.imagesrc %>" style="width: 300px;">
                    <form method="GET" action="/cart/<%= result.id%>">
                        <span class="shop-item-price p-0 m-0">
                            <h5 style="padding: 5px;">
                                Qty:
                                <%= result.quantity%>
                        </span>
                        <div class="def-number-input number-input safari_only" style="float: right;">
                            <input class="quantity" min="0" name="quantity" value="1" type="number"
                                style="width: 40px; text-align: center; font-size: 20px;">
                        </div>
                        </h5>
                        <div class="shop-item-details">
                            <span class="shop-item-price">
                                <h5>$<%= result.price%></h5>
                            </span>
                            <button class="btn btn-primary shop-item-button" type="submit">ADD TO CART</button>
                        </div>
                    </form>
                </div>
                <% }) %>
