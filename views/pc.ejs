<script src="store.js" async></script>

<body>
    <header>
        <div class="custom-nav-container d-flex align-items-center justify-content-between px-2 py-1 shadow-sm">
            <a class="text-clipped navbar-brand" href="/home">
                <i class="fal fa-bolt d-flex">
                    <span class="m-auto"> Electrified</span>
                </i>

            </a>
            <div class="main-nav-outer d-flex">
                <i class="fal fa-times text-clipped menu-close-icon d-lg-none"></i>
                <nav class="nav-main navbar navbar-light navbar-expand-lg text-center m-auto">
                    <div class="btn-toggle-nav"></div>
                    <ul class="navbar nav d-flex">
                        <li class="nav-item">
                            <a class="nav-link" href="/home">Home
                            </a>
                        </li class="nav-item">
                        <li>
                            <a class="nav-link" href="/mobile">Mobiles</a>
                        </li class="nav-item">
                        <li>
